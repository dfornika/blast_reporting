<?xml version="1.0"?>
<data_managers>
  <data_manager tool_file="data_manager_blast_reporting_data/data_manager/blast_reporting_data.xml" id="blast_reporting_data" version="0.1.3">
    <data_table name="blast_reporting">
      <output>
        <column name="value" />
        <column name="name" />
        <column name="path" output_ref="out_file">
          <move type="directory" relativize_symlinks="True">
            <target base="${GALAXY_DATA_MANAGER_DATA_PATH}">mentalist_databases/${value}</target>
          </move>
          <value_translation>${GALAXY_DATA_MANAGER_DATA_PATH}/mentalist_databases/${value}/${path}</value_translation>
          <value_translation type="function">abspath</value_translation>
        </column>
      </output>
    </data_table>
  </data_manager>
</data_managers>
